                <include layout="@layout/view_item_wishlist"></include>

                <include layout="@layout/view_item_wishlist"></include>

                <include layout="@layout/view_item_wishlist"></include>

                <include layout="@layout/view_item_wishlist"></include>

                <include layout="@layout/view_item_wishlist"></include>

                <include layout="@layout/view_item_wishlist"></include>

                <include layout="@layout/view_item_wishlist"></include>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>